function draw_ui()
	-- black background --
	rectfill(0, 96, 127, 127, 0)
	-- gray border --
	rect(0, 96, 127, 127, 6)
	
	print("♥♥♥", 4, 100, 8)
	
	-- item box --
	print("items", 105, 100, 7)
	rect(103, 98, 125, 125, 6)
	-- current time --
	spr(2, 105, 106) -- slot 0
	spr(2, 116, 106) -- slot 1
	spr(2, 105, 116) -- slot 2
	spr(2, 116, 116) -- slot 3
end
