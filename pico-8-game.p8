vel=1

function player_movement()
 dx=0
 dy=0

 l=tonum(btn(⬅️))
 r=tonum(btn(➡️))
 u=tonum(btn(⬆️))
 d=tonum(btn(⬇️))

 if l+r+u+d==1 then
  if l!=0 then
   dx-=1
  elseif r!=0 then
   dx+=1
  elseif u!=0 then
   dy-=1
  elseif d!=0 then
   dy+=1
  end
 end

 p.x+=dx
 p.y+=dy
end
